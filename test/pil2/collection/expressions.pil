subproof ExpressionTest(2**16) {

    col witness x[4],y;
    expr ex = x[0] + 2 * x[1] + 4 * x[2] + 2 ** 3 * x[3];
    x[0] + 2 * x[1] + 3 * x[2] + 5 * y === 0;
    ex === 0;

    int k = 10;
    expr a = 1;
    string s = "1";


    ex === 0;
    assert(k == 10);
    assert(a == 1);
    assert(string(1) == "1");
    assert(string(a) == "1");
    assert(string(a) == s);

    a = a + 1;
    dump(a);
    a = a * 10;
    dump(a);
    s = s + "hello";
    s = s + k;
    s = s + a;
}