include "../vadcop/std_sum.pil"
include "module.pil";

public mod;
public in1;
public in2;
public out;

subproof aggregate Fibonacci(2**16, 2**18) {

    subproofval aggregate(sum) gsum_fibonacci;

    col fixed L1 = [1,0...];
    col witness a,b;

    (b' - a) * (1 - L1') === 0;
    L1 * (b - in1) === 0;
    L1 * (a - in2) === 0;

    multiset_require(MODULE_ID, 1, [a*a + b*b, a'])
};


