
include "std.pil"
include "arith.pil"
air {
    pepe === 1;
}
namespace Main:: {

    pepe === L1 * gprod;
    col fixed CLK = [1,0:7]...;
    col witness empty;
    col witness a[2],b[2],c[2],d[2];

    for (col &r in [a,b,c,d]) {
        for (int i=0; i<2; ++i) {
            (r[i]'-r[i]) * (1-CLK) === 0;
        }
    }

    subair_multiset_requires(ARITH_ID, CLK, [...a,...b,...c,...d]);
}