subair Functions(2**23);

function func_with_once (col &p1) {
    once p1 === 314;
}

function func_with_once2 (int i) : int {
    println(i);
    once int j = 13;
    return i * j;
}

namespace Functions:: {

    col witness c1,c2;

    func_with_once(c1);
    func_with_once(c2);

    for (int i = 1; i < 5; ++i) {
        for (int j = i + 1; j < 5; ++j) {
            once c2 + c1 === i * j * 7;
        }
    }

    for (int i = 2; i < 5; ++i) {
        once {
            for (int j = i + 1; j < 5; ++j) {
                c2 + c1 === i * j * 11;
            }
        }
    }

    once {
        for (int i = 3; i < 5; ++i) {
            for (int j = i + 1; j < 7; ++j) {
                c2 + c1 === i * j;
            }
        }
    }

    c2 === func_with_once2(10);
    // func_with_once2(20); // generate an expected error
}