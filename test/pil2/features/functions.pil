subproof Functions(2**23) {

    function sum(int a, int b) : int {
        return a + b;
    }

    function fact(int k): int {
        if (k < 2) {
            return 1;
        }
        return k * fact(k - 1);
    }

    function procedure(int value) {
        col witness p1;

        p1 === value;
    }

    col witness c1,vec[10];

    c1 === fact(2);

    c1 === sum(10, 20);

    int res = sum(12, 15);

    c1 === res;

    c1 === vec[sum(2, 3)];
    c1 === (sum(3,5))'vec[sum(2, 3)];
    c1 === vec[sum(8, -3)]'(sum(6,4));

    c1 === fact(8);
    c1 === vec[fact(3)];
    c1 === c1'(fact(5));
    c1 === (fact(4))'c1;

    procedure(100);
    procedure(314);

    c1 === fact(4);
}